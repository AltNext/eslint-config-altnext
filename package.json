{
  "name": "eslint-config-altnext",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/altnext/eslint-config-altnext.git"
  },
  "author": "Altnext Gorillaz",
  "eslintIgnore": [
    "dist",
    "node_modules"
  ],
  "license": "MIT",
  "main": "./index.js",
  "files": [
    "**/*.js"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "tsc --build --clean",
    "deduplicate": "yarn-deduplicate -s fewer",
    "lint": "eslint --ext .ts . --cache",
    "prepare": "husky install",
    "prerelease": "yarn build",
    "release": "changeset publish",
    "type": "tsc --noEmit --noImplicitAny"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "eslint": "7.32.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-playwright": "0.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-unicorn": "36.0.0",
    "prettier": "2.4.1"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-jest-formatting": "3.0.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-playwright": "0.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unicorn": "36.0.0",
    "prettier": "2.4.1"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.1",
    "@changesets/cli": "2.17.0",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/eslint": "7.28.0",
    "cspell": "5.11.1",
    "eslint-config-altnext": "1.2.6",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "typescript": "4.4.3",
    "yarn-deduplicate": "3.1.0"
  },
  "lint-staged": {
    "**/*": [
      "cspell lint"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --quiet --cache"
    ]
  }
}
